{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "short_name": "cool browser",
  "version": "0.1",
  "description": "This is my cool extension.",
  "author": "@asachan",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Power my Page!"
    // "default_popup": "<any>.html"
  }
}